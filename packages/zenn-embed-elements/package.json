    "esbuild": "^0.17.8",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "katex": "^0.16.7",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.5"
  },
